{
  "name": "paterson-md",
  "version": "1.0.0",
  "description": "Advanced WhatsApp Bot with QR Pairing and Cloud Session Management",
  "main": "server.js",
  "scripts": {
    "start": "NODE_ENV=production pm2 start server.js --name PATERSON-MD --watch --no-daemon --time",
    "stop": "pm2 stop PATERSON-MD",
    "restart": "pm2 restart PATERSON-MD",
    "dev": "NODE_ENV=development node server.js",
    "test": "jest --coverage",
    "lint": "eslint ."
  },
  "keywords": [
    "whatsapp-bot",
    "baileys",
    "session-manager",
    "cloud-storage",
    "qr-pairing"
  ],
  "author": "Kervens Aubourg",
  "license": "MIT",
  "dependencies": {
    "@whiskeysockets/baileys": "^6.7.17",
    "async-mutex": "^0.4.1",
    "awesome-phonenumber": "^2.64.0",
    "body-parser": "^1.20.1",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "megajs": "^1.1.7",
    "pino": "^8.14.1",
    "pino-pretty": "^10.2.0",
    "pm2": "^5.3.1",
    "qrcode": "^1.5.3",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "eslint": "^8.48.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.28.1",
    "jest": "^29.6.4",
    "nodemon": "^3.0.1"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/PATERSON-MD/PATERSON-MD.git"
  },
  "bugs": {
    "url": "https://github.com/PATERSON-MD/PATERSON-MD/issues"
  },
  "homepage": "https://paterson-md.onrender.com"
